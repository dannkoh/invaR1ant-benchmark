@using spf-wca

target=custom.Collatz
shell=wcanalysis.WorstCaseAnalyzer

symbolic.min_int=1
symbolic.max_int=10
symbolic.dp=z3

symbolic.wc.policy.history.size=0
symbolic.worstcase.policy.inputsize=2
symbolic.worstcase.input.max=30

symbolic.worstcase.verbose=true
symbolic.worstcase.outputpath=${spf-wca}/custom/Collatz
symbolic.method=custom.Collatz.run(con)
